{
  "name": "datepicker",
  "version": "0.0.1",
  "description": "A lightweight accessible datepicker",
  "homepage": "http://code.viget.com/datepicker/",
  "repository": {
    "type": "git",
    "url": "git://github.com/vigetlabs/datepicker.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "rm -rf example && webpack --watch & browser-sync start --server --files example --index example/index.html",
    "prepublish": "rm -rf example && babel src/DatePicker -d dist & webpack src/standalone.js dist/window.DatePicker.js -p"
  },
  "keywords": [
    "calendar",
    "datepicker",
    "date",
    "month",
    "time",
    "vanilla",
    "js"
  ],
  "author": "Dan Tello",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.15",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-object-assign": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "browser-sync": "2.10.1",
    "webpack": "1.12.9"
  },
  "dependencies": {
    "delegate": "git://github.com/vigetlabs/delegate#3.0.1"
  }
}
